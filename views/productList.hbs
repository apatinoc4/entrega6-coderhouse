<table>
  <thead>
    <th>ID</th>
    <th>Nombre</th>
    <th>Precio</th>
    <th>Imagen</th>
  </thead>
  <tbody>
    {{#if listExists}}
      {{#each productList}}
        <tr>
          <td>
            {{this.id}}
          </td>
          <td>
            {{this.title}}
          </td>
          <td>
            {{this.price}}
          </td>
          <td>
            <img src={{this.thumbnail}} />
          </td>
        </tr>
      {{/each}}
    {{/if}}
  </tbody>
</table>
<input type="button" onclick="location.href='/';" value="Crear Producto" />